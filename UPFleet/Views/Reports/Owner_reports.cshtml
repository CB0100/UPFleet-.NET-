@model List<UPFleet.Models.Owner>
@{
    ViewData["Title"] = "Owner_reports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">

    <div class="function-btn" style="margin-bottom: 15px; margin-left: -10px;">
        <a href="@Url.Action("GoBack", "Maintenance")" id="backbtn" class="btn-primary">Back</a>
        <a id="prntbtn" title="Print This Report"><img src="~/wwwroot/image/printing.png"></a>
    </div>
    <div class="row" style="justify-content: center">

        <div class="col-lg-12 table-responsive">

            <table class=" table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>OwnerName</th>
                        <th>Company</th>
                        <th>Address1</th>
                        <th>Address2</th>
                        <th>City</th>
                        <th>State</th>
                        <th>Zip</th>
                        <th>Account</th>

                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.OwnerName</td>
                            <td>@item.Company</td>
                            <td>@item.Address1</td>
                            <td>@item.Address2</td>
                            <td>@item.City</td>
                            <td>@item.State</td>
                            <td>@item.Zip</td>
                            <td>@item.Account</td>
                        </tr>

                    }
                </tbody>
            </table>

        </div>

    </div>
</div>
@section Scripts{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        $(document).ready(function () {
            $("#prntbtn").click(function () {
                $(".function-btn").hide();
                window.print();
                $(".function-btn").show();
            });
        });
    </script>
}